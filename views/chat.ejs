<section id="chitchat">

    <div class="container-fluid chat" >

        <div class="card ">

            <div class="p-list">


                <div class="active" style="text-align: center">

                    <h4>
                        Active Members
                    </h4>
                    <hr>


                </div>

                <div class="members scrollbar-deep-purple bordered-deep-purple thin">

                    <ul class="list-group">
                        <li class="list-group-item">Cras justo odio</li>
